{
  "dashboard": {
    "id": null,
    "title": "HASIVU Executive Overview - Multi-School Platform Performance",
    "tags": ["hasivu", "executive", "overview", "multi-tenant"],
    "style": "dark",
    "timezone": "Asia/Kolkata",
    "panels": [
      {
        "id": 1,
        "title": "Platform Health Score",
        "type": "stat",
        "targets": [
          {
            "expr": "avg((\n  (avg(up{job=~\"hasivu-.*\"}) * 100) +\n  (avg(1 - (rate(hasivu_errors_total[5m]) / rate(hasivu_requests_total[5m]))) * 100) +\n  (avg(1 - (hasivu_response_time_seconds{quantile=\"0.95\"} / 5)) * 100)\n) / 3)",
            "legendFormat": "Overall Health Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 85 },
                { "color": "green", "value": 95 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Active Schools",
        "type": "stat",
        "targets": [
          {
            "expr": "count(count by (school_id)(up{job=~\"hasivu-.*\",school_id!=\"\"}))",
            "legendFormat": "Total Active Schools",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": { "mode": "palette-classic" }
          }
        },
        "gridPos": { "h": 4, "w": 3, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "Daily Meals Served",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(school_daily_meals_served_total[24h]))",
            "legendFormat": "Total Meals Today",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": { "mode": "palette-classic" }
          }
        },
        "gridPos": { "h": 4, "w": 3, "x": 9, "y": 0 }
      },
      {
        "id": 4,
        "title": "Platform Revenue (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(billing_monthly_revenue_dollars[24h]))",
            "legendFormat": "Daily Revenue",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": { "mode": "palette-classic" }
          }
        },
        "gridPos": { "h": 4, "w": 3, "x": 12, "y": 0 }
      },
      {
        "id": 5,
        "title": "Critical Alerts",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(ALERTS{severity=\"critical\",alertstate=\"firing\"})",
            "legendFormat": "Active Critical Alerts",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 3 }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 3, "x": 15, "y": 0 }
      },
      {
        "id": 6,
        "title": "System Performance Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(hasivu_response_time_seconds{quantile=\"0.95\"})",
            "legendFormat": "95th Percentile Response Time",
            "refId": "A"
          },
          {
            "expr": "rate(hasivu_requests_total[5m])",
            "legendFormat": "Request Rate",
            "refId": "B"
          },
          {
            "expr": "rate(hasivu_errors_total[5m]) / rate(hasivu_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 6, "y": 4 }
      },
      {
        "id": 7,
        "title": "School Performance Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "count by (performance_tier)(\n  (\n    avg by (school_id)(\n      school_meal_satisfaction_score\n    ) > 0.95\n  ) * on() group_left() label_replace({}, \"performance_tier\", \"Excellent\", \"\", \"\")\n  or\n  (\n    avg by (school_id)(\n      school_meal_satisfaction_score\n    ) > 0.85\n  ) * on() group_left() label_replace({}, \"performance_tier\", \"Good\", \"\", \"\")\n  or\n  (\n    avg by (school_id)(\n      school_meal_satisfaction_score\n    ) > 0.75\n  ) * on() group_left() label_replace({}, \"performance_tier\", \"Average\", \"\", \"\")\n  or\n  label_replace({}, \"performance_tier\", \"Needs Improvement\", \"\", \"\")\n)",
            "legendFormat": "{{performance_tier}}",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 8,
        "title": "Multi-Tenant Resource Utilization",
        "type": "bargauge",
        "targets": [
          {
            "expr": "topk(10, avg by (school_id)(billing_cost_per_school_dollars))",
            "legendFormat": "{{school_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "min": 0,
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 500 },
                { "color": "red", "value": 1000 }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 9,
        "title": "AI Analytics Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(ml_model_predictions_total[5m])",
            "legendFormat": "ML Predictions/sec",
            "refId": "A"
          },
          {
            "expr": "avg(ml_model_accuracy_score)",
            "legendFormat": "Model Accuracy",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ml_inference_time_milliseconds_bucket[5m]))",
            "legendFormat": "95th Percentile Inference Time",
            "refId": "C"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 10,
        "title": "Vendor Marketplace Efficiency",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(vendor_procurement_efficiency_ratio)",
            "legendFormat": "Procurement Efficiency",
            "refId": "A"
          },
          {
            "expr": "avg(vendor_supply_chain_reliability_score)",
            "legendFormat": "Supply Chain Reliability",
            "refId": "B"
          },
          {
            "expr": "sum(rate(vendor_ai_optimization_recommendations_total[1h]))",
            "legendFormat": "AI Recommendations/hour",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": { "h": 4, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 11,
        "title": "Security and Compliance Status",
        "type": "table",
        "targets": [
          {
            "expr": "security_gdpr_compliance_violations_total",
            "legendFormat": "GDPR Violations",
            "refId": "A"
          },
          {
            "expr": "security_failed_login_attempts_total",
            "legendFormat": "Failed Login Attempts",
            "refId": "B"
          },
          {
            "expr": "privacy_data_retention_violations_total",
            "legendFormat": "Data Retention Violations",
            "refId": "C"
          },
          {
            "expr": "security_vulnerability_scan_score",
            "legendFormat": "Vulnerability Scan Score",
            "refId": "D"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          }
        ],
        "gridPos": { "h": 6, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": 12,
        "title": "Database Performance Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(pg_connections)",
            "legendFormat": "Active DB Connections",
            "refId": "A"
          },
          {
            "expr": "rate(pg_stat_database_tup_inserted[5m])",
            "legendFormat": "Insert Rate",
            "refId": "B"
          },
          {
            "expr": "rate(pg_stat_database_tup_updated[5m])",
            "legendFormat": "Update Rate",
            "refId": "C"
          },
          {
            "expr": "avg(pg_stat_replication_lag_seconds)",
            "legendFormat": "Replication Lag",
            "refId": "D"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 }
      },
      {
        "id": 13,
        "title": "Infrastructure Cost Analysis",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(billing_monthly_revenue_dollars) / sum(billing_cost_per_school_dollars) * 100",
            "legendFormat": "Profit Margin %",
            "refId": "A"
          },
          {
            "expr": "sum(rate(billing_cost_per_school_dollars[1h]))",
            "legendFormat": "Hourly Cost Rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 }
      },
      {
        "id": 14,
        "title": "Real-Time School Activity Heatmap",
        "type": "heatmap",
        "targets": [
          {
            "expr": "sum by (school_id)(rate(school_daily_meals_served_total[5m]))",
            "legendFormat": "{{school_id}}",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 }
      },
      {
        "id": 15,
        "title": "Federated Learning Network Status",
        "type": "nodeGraph",
        "targets": [
          {
            "expr": "sum by (source_school, target_school)(analytics_federated_learning_updates_total)",
            "legendFormat": "{{source_school}} → {{target_school}}",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 10, "w": 24, "x": 0, "y": 36 }
      },
      {
        "id": 16,
        "title": "SLA Compliance Dashboard",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(hasivu_response_time_seconds{quantile=\"0.95\"}) < bool 2",
            "legendFormat": "Response Time SLA",
            "refId": "A"
          },
          {
            "expr": "avg(up{job=~\"hasivu-.*\"}) >= bool 0.999",
            "legendFormat": "Availability SLA",
            "refId": "B"
          },
          {
            "expr": "avg(1 - (rate(hasivu_errors_total[5m]) / rate(hasivu_requests_total[5m]))) >= bool 0.995",
            "legendFormat": "Error Rate SLA",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              { "type": "value", "value": "0", "text": "VIOLATED" },
              { "type": "value", "value": "1", "text": "COMPLIANT" }
            ],
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            }
          }
        },
        "gridPos": { "h": 6, "w": 24, "x": 0, "y": 46 }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "refresh": "30s",
    "schemaVersion": 36,
    "version": 1,
    "links": [
      {
        "title": "HASIVU Technical Dashboard",
        "url": "/d/hasivu-technical-overview",
        "type": "dashboards"
      },
      {
        "title": "School-Specific Analytics",
        "url": "/d/hasivu-school-analytics",
        "type": "dashboards"
      },
      {
        "title": "Security Monitoring",
        "url": "/d/hasivu-security-monitoring",
        "type": "dashboards"
      }
    ]
  }
}
